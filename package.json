  "bugs": {
    "url": "https://github.com/franvarney/franvarney/issues"
  },
  "homepage": "https://github.com/franvarney/franvarney#readme",
  "devDependencies": {
    "babel-core": "6.4.0",
    "babel-loader": "6.2.1",
    "babel-preset-es2015": "6.3.13",
    "babel-preset-react": "6.3.13",
    "babel-preset-stage-0": "6.3.13",
    "bootstrap-styl": "4.0.5",
    "css-loader": "0.16.0",
    "d3": "3.5.17",
    "extract-text-webpack-plugin": "0.8.2",
    "file-loader": "0.8.5",
    "google-map-react": "0.13.0",
