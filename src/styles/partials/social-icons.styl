.social-container {
  bottom: 0
  left: 0
  margin: 25px auto
  position: absolute
  right: 0
  text-align: center

  ul {
    list-style-type: none
    height: 30px
    margin: 0 0 0 10px
    padding: 0
    position: relative
  }

  li {
    display: inline-block
    height: 100%
    margin: 0 5px
    padding: 0
    position: relative
    text-align: center
    width: auto

    a {
      display: inline-block
      height: 100%
    }

    img {
      height: 100%
      margin: 0
      width: auto
    }
  }
}
